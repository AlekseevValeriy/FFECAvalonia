<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        x:Class="FFECAvalonia.Views.DialogView.TextEnterDIalogView"
        xmlns:vm="clr-namespace:FFECAvalonia.ViewModels"
        x:DataType="vm:TextEnterDIalogViewModel"
        WindowStartupLocation="CenterOwner"
        Width="220" 
        Height="130"
        Title="Please enter layout name">
  <Grid RowDefinitions="*, *" ColumnDefinitions="100,*">
    <TextBlock Grid.Column="0" Grid.Row="0" Text="Название"></TextBlock>
    <TextBox Grid.Column="1" Grid.Row="0" x:Name="InputTextBox"></TextBox>
    <Button Grid.Column="0" Grid.Row="1" Content="Cance" x:Name="CanceButton"></Button>
    <Button Grid.Column="1" Grid.Row="1" Content="Ok" x:Name="OkButton"></Button>
  </Grid>
</Window>
